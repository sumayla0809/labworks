#include <iostream>
using namespace std;

void printFibonacci(int n) {
    int first = 0, second = 1, next;

    if (n >= 1) {
        cout << first << " ";
    }
    if (n >= 2) {
        cout << second << " ";
    }

    for (int i = 3; i <= n; i++) {
        next = first + second;
        cout << next << " ";
        first = second;
        second = next;
    }

    cout << endl;
}

int main() {
    int n;

    cout << "Enter the value of n: ";
    cin >> n;

    cout << "Fibonacci series up to " << n << "th term: ";
    printFibonacci(n);

    return 0;
}
